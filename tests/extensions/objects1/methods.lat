class A {
    int x, y;

    void printValues()
    {
        printInt(x);
        printInt(y);
    }
}

class B extends A {
    int z;

    void printSum()
    {
        int n = x + y + z;

        printValues();
        printString("==");
        printInt(n);
    }
}

int main () {
    B b = new B;

    b.x = 10;
    b.y = 11;
    b.z = 12;

    b.printSum();

    return 0;
}
